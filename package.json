{
  "name": "pesmomat",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:watch": "nodemon --watch config --exec \"nuxt --dotenv .env\"",
    "dev:dev": "nuxt --dotenv .env",
    "dev:rpi": "nuxt --dotenv .env.rpi",
    "dev:prod": "nuxt --dotenv .env.prod",
    "build:dev": "nuxt build --dotenv .env",
    "build:rpi": "nuxt build --dotenv .env.rpi",
    "build:prod": "nuxt build --dotenv .env.prod",
    "start:dev": "nuxt start --dotenv .env",
    "start:rpi": "nuxt start --dotenv .env.rpi",
    "start:prod": "npx cross-env ENV=production ENABLE_STATS=true ENABLE_GENERATE=false ADMIN=false nuxt start --dotenv .env.prod",
    "start:prod2": "npx cross-env ENV=production ENABLE_STATS=true ENABLE_GENERATE=true ADMIN=false nuxt start",
    "generate": "nuxt generate --dotenv .env"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/i18n": "^7.3.0",
    "@nuxtjs/toast": "^3.3.1",
    "acorn": "^6.4.2",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "core-js": "^3.8.3",
    "cross-env": "^7.0.3",
    "jsonwebtoken": "^9.0.0",
    "nuxt": "^2.15.8",
    "qrcode.vue": "^1.7.0",
    "simple-keyboard": "^3.6.18",
    "v-click-outside": "^3.2.0",
    "vee-validate": "^3.4.15",
    "vue2-touch-events": "^3.2.3"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/google-fonts": "^3.0.2",
    "nodemon": "^3.0.1",
    "sass": "^1.44.0",
    "sass-loader": "^10.4.1"
  },
  "overrides": {
    "autoprefixer": "10.4.5",
    "vue": "^2.7.15",
    "vue-server-renderer": "^2.7.15",
    "vue-template-compiler": "^2.7.15",
    "glob-parent": "6.0.2",
    "nth-check": "2.1.1"
  }
}
